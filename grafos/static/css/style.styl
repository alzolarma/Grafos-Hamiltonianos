@import url('http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700')

@font-face 
  font-family 'Roboto Slab', serif
  src url("/../static/fonts/Roboto_Slab/RobotoSlab-Regular.ttf")
  font-weight 400

html
  height 100%
  width 100%
  padding: 0
  margin 0  
  font-family 'Roboto Slab', serif

body
  background-image url("/../static/img/b-sidebar-back.png")

  height 100%
  width 100%
  font-size: 0.9em
  color #333

*
  margin: 0
  padding: 0

a
  text-decoration: none
  font-weight: bold
  color: #FFFFFF

p
  color: #777

h1,
h2,
h3,
h4,
h5
  color: #777

i

input
  height: 25px
  border: 1px solid #ccc
  border-radius: 0.25em
  outline: 0px

*,
*:before,
*:after
  -moz-box-sizing: border-box
  -ms-box-sizing: border-box
  -o-box-sizing: border-box
  -webkit-box-sizing: border-box
  box-sizing: border-box

/*----------------Background-----------*/
header
  background orange
  border-bottom: 1px solid #eee

/*---------------HEADER---------------*/
header
  width: 80%
  height: 70px
  margin: 0 auto

section
  width: 100%
  height: 100%
  background-position: fixed

/*--------------Navegacion------------*/
.header_nav_left
  width 100%
  height 60px
  border-bottom 1px inset #e0e0e0
  padding-top 1em
  position absolute
  display         flex
  display -webkit-flex  
  background #0B0B0B
  background #0B0B0B url("/../static/img/bg.png")
  
.header_nav_left .central
  margin 0 auto
  width 900px
  height auto

.header_nav_left .central div
  display inline

.header_nav_left .central div a
  text-decoration none
  font-size 1.5em
  font-weight 400
  color #fff

.header_nav_left form
  display inline
  padding-top 0.7em
  margin-left 1em

.header_nav_left form input
  width: 300px
  padding: 1em

.header_nav_left ul
  display: inline
  margin: 0 auto
  width auto
  float right

.header_nav_left ul li
  display: inline-block
  padding: 0em 0em 0.2em 1em

.header_nav_left ul li a
  border: 0px solid #fff
  border-color: transparent
  -webkit-transition: all 0.3s ease
  transition: all 0.3s ease
  font-weight 500
  padding: 0.5em 1em 0.5em 1em
  border-radius: 0.3em
  background: #dbdbdb;
  background-image: -webkit-linear-gradient(top, #dbdbdb, #a8a8a8);
  background-image: -moz-linear-gradient(top, #dbdbdb, #a8a8a8);
  background-image: -ms-linear-gradient(top, #dbdbdb, #a8a8a8);
  background-image: -o-linear-gradient(top, #dbdbdb, #a8a8a8);
  background-image: linear-gradient(to bottom, #dbdbdb, #a8a8a8);

.header_nav_left ul li a:hover,
.header_nav_left ul li .current
 /* background -webkit-linear-gradient(#DFDFDF, #C3C3C3)*/
  border-radius: 0.3em
  color #fff
  background: #a8a8a8;
  background: -webkit-linear-gradient(top, #a8a8a8, #dbdbdb);
  background: -moz-linear-gradient(top, #a8a8a8, #dbdbdb);
  background: -ms-linear-gradient(top, #a8a8a8, #dbdbdb);
  background: -o-linear-gradient(top, #a8a8a8, #dbdbdb);
  background: linear-gradient(to bottom, #a8a8a8, #dbdbdb);

.header_nav_left #login
  float: right
  padding: 0em 1em 0.5em 1em
.header_nav_left #login a
  float: right
  border: 1px solid #ddd
  border-radius: 0.3em
  padding: 0.5em 1.5em 0.5em 1.5em
  background -webkit-linear-gradient(#f5f5f5, #eee)
  background -o-linear-gradient(#f5f5f5, #eee)
  background linear-gradient(#f5f5f5, #eee)
  font-weight 400

.header_nav_left #login a:hover
  border: 1px solid #ccc
  background -webkit-linear-gradient(#eee, #f5f5f5)
  background -o-linear-gradient(#eee, #f5f5f5)
  background linear-gradient(#eee, #f5f5f5)

.header_nav_right
  padding: 1em 0em 1em 2em
  float: right
  display: inline-block

.header_nav_right a
  text-decoration: none
  color: #333

.buscar input
  padding: 0.5em

.img_user
  position: absolute
  display: inline
  margin-left: -30px
  width: 25px
  height: 25px
  border-radius: 2px
  margin-top: -5px

.center
  width: 65%
  margin: 0 auto
  padding: 0

.left
  float: left
  width: 47%

.right
  float: right
  width: 40%

.iconos
  height: 200px
  margin-bottom: 3em
  text-align: center

.iconos div
  padding-top: 28px
  border: 1px solid #eee
  width: 100px
  height: 100px
  border-radius: 100%
  margin: 0 auto

.iconos h3
  font-size: 1.6em

.iconos h3,
.iconos p
  margin: 0 auto
  margin-top: 1em
  text-align: center

.iconos .fa:hover
  color: #BBAC40
  font-size: 3.2em
  padding-top: -10px

#info h2
  text-align: center
  padding-top: 100px
  padding-bottom: 2em

.iconos .fa
  -webkit-transition: all 0.5s ease
  transition: all 0.5s ease
  font-size: 3em
  margin: 0 auto
  color: #D8C858

